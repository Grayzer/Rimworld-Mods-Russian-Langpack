<?xml version="1.0" encoding="utf-8" ?>
<Patch>

<!-- ===Additional Icons=== -->

	<Operation Class="PatchOperationSequence">
    <success>Always</success>

	  <operations>
		<li Class="PatchOperationTest">
		<xpath>Defs/ThingDef[defName = "FSFImplantHazardModule"]</xpath>
		</li>

		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "FSFImplantHazardModule"]</xpath>
			<value>
				<isTechHediff>true</isTechHediff>
				<graphicData Inherit="False">
				  <texPath>Things/Item/Health/HealthItemFSFImplantHazardModule</texPath>
				  <graphicClass>Graphic_StackCount</graphicClass>
				  <color>(200,160,130)</color>
				</graphicData>
			</value>
		</li>


		<li Class="PatchOperationAdd">
			<xpath>*/ThingDef[defName = "FSFImplantSubdermalArmor"]</xpath>
			<value>
				<isTechHediff>true</isTechHediff>
				<graphicData Inherit="False">
				  <texPath>Things/Item/Health/HealthItemFSFImplantSubdermalArmor</texPath>
				  <graphicClass>Graphic_StackCount</graphicClass>
				  <color>(200,160,130)</color>
				</graphicData>
			</value>
		</li>

	</operations>
	</Operation>



</Patch>
