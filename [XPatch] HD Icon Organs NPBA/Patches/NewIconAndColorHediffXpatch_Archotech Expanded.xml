<!-- Implant -->
<!-- ArchotechBrainMelder -->
      <li Class="PatchOperationConditional">
        <xpath>*/ThingDef[defName = "ArchotechBrainMelder"]/graphicData</xpath>
        <match Class="PatchOperationReplace">
          <xpath>*/ThingDef[defName = "ArchotechBrainMelder"]/graphicData</xpath>
          <value>
            <graphicData Inherit="False">
              <texPath>Things/Item/Health/HealthItemBrainMelder</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 153, 102)</color>
            </graphicData>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>*/ThingDef[defName = "ArchotechBrainMelder"]</xpath>
