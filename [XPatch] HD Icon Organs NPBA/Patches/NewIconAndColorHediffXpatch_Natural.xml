          <xpath>*/ThingDef[defName = "Heart"]/graphicData</xpath>
          <value>
            <graphicData Inherit="False">
              <texPath>Things/Item/Health/HealthItemHeart</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 51, 153)</color>
            </graphicData>
