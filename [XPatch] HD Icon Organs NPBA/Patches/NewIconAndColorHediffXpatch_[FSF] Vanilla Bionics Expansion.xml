<Operation Class="PatchOperationFindMod">
<mods>
<li>[FSF] Vanilla Bionics Expansion</li>
</mods>
  <match Class="PatchOperationSequence">
    <success>Always</success>
    <operations>
<!-- Prostetic -->
<!-- FSFArtificialNose -->
      <li Class="PatchOperationConditional">
        <xpath>*/ThingDef[defName = "FSFArtificialNose"]/graphicData</xpath>
        <match Class="PatchOperationReplace">
          <xpath>*/ThingDef[defName = "FSFArtificialNose"]/graphicData</xpath>
          <value>
            <graphicData Inherit="False">
              <texPath>Things/Item/Health/HealthItemNose</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 153, 102)</color>
            </graphicData>
          </value>
        </match>
        <nomatch Class="PatchOperationAdd">
          <xpath>*/ThingDef[defName = "FSFArtificialNose"]</xpath>
