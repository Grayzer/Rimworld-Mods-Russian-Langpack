          <xpath>*/ThingDef[defName = "PegDick"]/graphicData</xpath>
          <value>
            <graphicData Inherit="False">
              <texPath>Things/Item/Health/HealthItemGenMale</texPath>
              <graphicClass>Graphic_StackCount</graphicClass>
              <color>(255, 153, 102)</color>
