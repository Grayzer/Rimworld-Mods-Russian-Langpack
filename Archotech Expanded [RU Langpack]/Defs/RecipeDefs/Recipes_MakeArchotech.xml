<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RecipeDef Abstract="True" Name="MakePrivates">
		<workSpeedStat>SmithingSpeed</workSpeedStat>
		<unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		<workSkill>Crafting</workSkill>
	</RecipeDef>

	<RecipeDef ParentName="MakePrivates">
		<defName>Make_Archotech_Breasts</defName>
		<label>craft Archotech Breasts</label>
		<description>Crafts a Archotech Breasts from Nanomachinery, Picomachinery, and Silver.</description>
		<jobString>Crafting Archotech Breasts.</jobString>
		<soundWorking>Recipe_Machining</soundWorking>
		<workAmount>5000</workAmount>
		    <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
		<skillRequirements>
			<Crafting>14</Crafting>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Nanomachinery</li>
					</thingDefs>
				</filter>
				<count>50</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Picomachinery</li>
					</thingDefs>
				</filter>
				<count>25</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Silver</li>
					</thingDefs>
				</filter>
				<count>500</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Nanomachinery</li>
				<li>Picomachinery</li>
				<li>Silver</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<ArchotechBreasts>1</ArchotechBreasts>
		</products>
		<researchPrerequisite>ArchotechMimicry</researchPrerequisite>
		<recipeUsers>
			<li>BasicArchotechWorkbench</li>
		</recipeUsers>

	</RecipeDef>

